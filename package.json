{
  "name": "servidor",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node --experimental-modules ./server/bin/www.js",
    "begin": "nodemon"
  },
  "nodemonConfig": {
    "exec": "npm start",
    "watch": [
      "server/*"
    ],
    "ignore": [
      "**/__tests__/**",
      "*.test.js",
      "*.spec.js"
    ]
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "morgan": "^1.9.1",
    "mysql": "^2.18.1",
    "npm-run-all": "^4.1.5",
    "pug": "^3.0.1",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "nodemon": "^2.0.4"
  }
}
